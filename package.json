{
  "name": "docs-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "docs:generate": "ts-node --project tsconfig.node.json src/cli/docs-generator-cli.ts generate",
